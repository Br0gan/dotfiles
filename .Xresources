! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
!
! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   argb:00000000, #7CAFC2, argb:11FFFFFF, #1aa,#333
! Color scheme for urgent row
rofi.color-urgent:                   argb:00000000, #f99, argb:11FFFFFF, #f99,#333
! Color scheme for active row
rofi.color-active:                   argb:00000000, #7CAFC2, argb:11FFFFFF, #7CAFC2, #333
! Color scheme window
rofi.color-window:                   argb:ee333333, argb:ee333333, #1aa
! Separator style (none, dash, solid)
rofi.separator-style:                none
